{
  "name": "country-currency-anyfin",
  "version": "1.0.0",
  "description": "Currency Exchange Rate",
  "main": "index.js",
  "author": "Shruti Chandran",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/src/app.js",
    "build:watch": "tsc -w",
    "test": "jest --forceExit --collect-coverage",
    "test:watch": "jest --watch",
    "test:eslint": "eslint src/ --ext .ts,.js",
    "test:jenkins": "npm run test:prettier && npm run test:eslint && npm run test",
    "test:prettier": "prettier --list-different \"lib/src/**/*.{js,ts,md}\"",
    "fix:prettier": "prettier --single-quote --write \"lib/src/**/*.{js,ts,md}\"",
    "fix:eslint": "eslint --fix"
  },
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^3.6.2",
    "axios": "^0.25.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "graphql-tools": "^4.0.8",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "winston": "^3.5.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "eslint": "^8.8.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^16.3.0",
    "jest": "^27.5.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
